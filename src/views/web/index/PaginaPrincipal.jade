extends ../../layout 
block content  
  div(class="content-wrapper")
    section(class="content-header")
      h1 Inicio
        small Página Principal
      ol(class="breadcrumb")
        li
          a(href="#") 
            i(class="fa fa-dashboard") Inicio
        li(class="active") Página Principal
    section(class="content")
      div(class="row")
        div(class="col-lg-3 col-xs-6")
          div(class="small-box bg-aqua")
            div(class="inner")
              h3 150
              p Stock de Insumos
            div(class="icon")
              i(class="ion ion-bag")
            a(href="#" class="small-box-footer") Más Información
              i(class="fa fa-arrow-circle-right")
        div(class="col-lg-3 col-xs-6")
          div(class="small-box bg-green")
            div(class="inner")
              h3 53 
                sup(style="font-size: 20px") %
              p Promedio de Pesajes
            div(class="icon")
              i(class="ion ion-stats-bars")
            a(href="#" class="small-box-footer") Más Información
              i(class="fa fa-arrow-circle-right")
        div(class="col-lg-3 col-xs-6")
          <!-- small box -->
          div(class="small-box bg-yellow")
            div(class="inner")
              h3 44
              p Animales Sanitados
            div(class="icon")
              i(class="ion ion-person-add")
            a(href="#" class="small-box-footer") Más Información 
              i(class="fa fa-arrow-circle-right")
        div(class="col-lg-3 col-xs-6")
          div(class="small-box bg-red")
            div(class="inner")
              h3 65
              p Promedio de Consumo 
            div(class="icon")
              i(class="ion ion-pie-graph")
            a(href="#" class="small-box-footer") Más Información 
              i(class="fa fa-arrow-circle-right")
      div(class="row")
        section(class="col-lg-7 connectedSortable")
          div(class="nav-tabs-custom")
            ul(class="nav nav-tabs pull-right")
              li(class="active") 
                a(href="#revenue-chart" data-toggle="tab") Área
              li 
                a(href="#sales-chart" data-toggle="tab") Donut
              li(class="pull-left header") 
                i(class="fa fa-inbox") Sales
            div(class="tab-content no-padding")
              div(class="chart tab-pane active" id="revenue-chart" style="position: relative; height: 300px;")
              div(class="chart tab-pane" id="sales-chart" style="position: relative; height: 300px;")
          div(class="box box-success")
            div(class="box-header")
              i(class="fa fa-comments-o")
              h3(class="box-title") Chat
              div(class="box-tools pull-right" data-toggle="tooltip" title="Status")
                div(class="btn-group" data-toggle="btn-toggle")
                  button(type="button" class="btn btn-default btn-sm active")
                    i(class="fa fa-square text-green")
                  button(type="button" class="btn btn-default btn-sm")
                    i(class="fa fa-square text-red")
            div(class="box-body chat" id="chat-box")
              div(class="item")
                img(src="dist/img/user4-128x128.jpg" alt="user image" class="online")
                p(class="message") I would like to meet you to discuss the latest news about the arrival of the new theme. They say it is going to be one the best themes on the market
                  a(href="#" class="name") Mike Doe
                    small(class="text-muted pull-right")
                      i(class="fa fa-clock-o") 2:15  
                div(class="attachment")
                  h4 Attachments:
                  p(class="filename") Theme-thumbnail-image.jpg
                  div(class="pull-right")
                    button(type="button" class="btn btn-primary btn-sm btn-flat") Open
              div(class="item")
                img(src="dist/img/user3-128x128.jpg" alt="user image" class="offline")
                p(class="message") I would like to meet you to discuss the latest news about the arrival of the new theme. They say it is going to be one the best themes on the market
                  a(href="#" class="name")  Alexander Pierce
                    small(class="text-muted pull-right")
                      i(class="fa fa-clock-o") 5:15
              div(class="item")
                img(src="dist/img/user2-160x160.jpg" alt="user image" class="offline")
                p(class="message") I would like to meet you to discuss the latest news about the arrival of the new theme. They say it is going to be one the best themes on the market
                  a(href="#" class="name") Susan Doe
                    small(class="text-muted pull-right") 
                      i(class="fa fa-clock-o") 5:30
            div(class="box-footer")
              div(class="input-group")
                input(class="form-control" placeholder="Type message...")
                div(class="input-group-btn")
                  button(type="button" class="btn btn-success")
                    i(class="fa fa-plus")
          div(class="box box-primary")
            div(class="box-header")
              i(class="ion ion-clipboard")
              h3(class="box-title") To Do List
              div(class="box-tools pull-right")
                ul(class="pagination pagination-sm inline")
                  li
                    a(href="#") &laquo;
                  li
                    a(href="#") 1
                  li
                    a(href="#") 2
                  li
                    a(href="#") 3
                  li
                    a(href="#") &raquo;
            div(class="box-body")
              ul(class="todo-list")
                li
                  span(class="handle")
                    i(class="fa fa-ellipsis-v")
                    i(class="fa fa-ellipsis-v")
                  input(type="checkbox" value="") 
                  span(class="text") Design a nice theme
                  small(class="label label-danger")
                    i(class="fa fa-clock-o") 2 mins
                  div(class="tools")
                    i(class="fa fa-edit")
                    i(class="fa fa-trash-o")
                li
                  span(class="handle")
                    i(class="fa fa-ellipsis-v")
                    i(class="fa fa-ellipsis-v")
                  input(type="checkbox" value="") 
                  span(class="text") Make the theme responsive
                  small(class="label label-info") 
                    i(class="fa fa-clock-o") 4 hours
                  div(class="tools") 
                    i(class="fa fa-edit")
                    i(class="fa fa-trash-o")
                li
                  span(class="handle")
                    i(class="fa fa-ellipsis-v")
                    i(class="fa fa-ellipsis-v")
                  input(type="checkbox" value="") 
                  span(class="text") Let theme shine like a star
                  small(class="label label-warning") 
                    i(class="fa fa-clock-o") 1 day
                  div(class="tools") 
                    i(class="fa fa-edit")
                    i(class="fa fa-trash-o")                  
                li
                  span(class="handle")
                    i(class="fa fa-ellipsis-v")
                    i(class="fa fa-ellipsis-v")
                  input(type="checkbox" value="") 
                  span(class="text") Let theme shine like a star
                  small(class="label label-success") 
                    i(class="fa fa-clock-o") 3 days
                  div(class="tools") 
                    i(class="fa fa-edit")
                    i(class="fa fa-trash-o")
                li
                  span(class="handle")
                    i(class="fa fa-ellipsis-v")
                    i(class="fa fa-ellipsis-v")
                  input(type="checkbox" value="") 
                  span(class="text") Check your messages and notifications
                  small(class="label label-primary") 
                    i(class="fa fa-clock-o") 1 week
                  div(class="tools") 
                    i(class="fa fa-edit")
                    i(class="fa fa-trash-o")
                li
                  span(class="handle")
                    i(class="fa fa-ellipsis-v")
                    i(class="fa fa-ellipsis-v")
                  input(type="checkbox" value="") 
                  span(class="text") Let theme shine like a star
                  small(class="label label-default")
                    i(class="fa fa-clock-o") 1 month
                  div(class="tools")
                    i(class="fa fa-edit")
                    i(class="fa fa-trash-o")
            div(class="box-footer clearfix no-border")
              button(type="button" class="btn btn-default pull-right")
                i(class="fa fa-plus") Add item
          div(class="box box-info")
            div(class="box-header")
              i(class="fa fa-envelope")
              h3(class="box-title") Quick Email
              div(class="pull-right box-tools")
                button(type="button" class="btn btn-info btn-sm" data-widget="remove" data-toggle="tooltip" title="Remove")
                  i(class="fa fa-times")
            div(class="box-body")
              form(action="#" method="post")
                div(class="form-group")
                  input(type="email" class="form-control" name="emailto" placeholder="Email to:")
                div(class="form-group")
                  input(type="text" class="form-control" name="subject" placeholder="Subject")
                div
                  textarea(class="textarea" placeholder="Message" style="width: 100%; height: 125px; font-size: 14px; line-height: 18px; border: 1px solid #dddddd; padding: 10px;")
            div(class="box-footer clearfix") 
              button(type="button" class="pull-right btn btn-default" id="sendEmail") Send
                i(class="fa fa-arrow-circle-right")
        section(class="col-lg-5 connectedSortable")
          div(class="box box-solid bg-light-blue-gradient")
            div(class="box-header")
              div(class="pull-right box-tools")
                button(type="button" class="btn btn-primary btn-sm daterange pull-right" data-toggle="tooltip" title="Date range")
                  i(class="fa fa-calendar")
                button(type="button" class="btn btn-primary btn-sm pull-right" data-widget="collapse" data-toggle="tooltip" title="Collapse" style="margin-right: 5px;")
                  i(class="fa fa-minus")
              i(class="fa fa-map-marker")
              h3(class="box-title") Visitors
            div(class="box-body")
              div(id="world-map" style="height: 250px; width: 100%;")
            div(class="box-footer no-border")
              div(class="row")
                div(class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4")
                  div(id="sparkline-1")
                  div(class="knob-label") Visitors
                div(class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4")
                  div(id="sparkline-2")
                  div(class="knob-label") Online
                div(class="col-xs-4 text-center")
                  div(id="sparkline-3")
                  div(class="knob-label") Exists
          div(class="box box-solid bg-teal-gradient")
            div(class="box-header")
              i(class="fa fa-th")
              h3(class="box-title") Sales Graph
              div(class="box-tools pull-right")
                button(type="button" class="btn bg-teal btn-sm" data-widget="collapse")
                  i(class="fa fa-minus")
                button(type="button" class="btn bg-teal btn-sm" data-widget="remove")
                  i(class="fa fa-times")
            div(class="box-body border-radius-none")
              div(class="chart" id="line-chart" style="height: 250px;")
            div(class="box-footer no-border")
              div(class="row")
                div(class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4")
                  input(type="text" class="knob" data-readonly="true" value="20" data-width="60" data-height="60" data-fgColor="#39CCCC")
                  div(class="knob-label") Mail-Orders
                div(class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4")
                  input(type="text" class="knob" data-readonly="true" value="50" data-width="60" data-height="60" data-fgColor="#39CCCC")
                  div(class="knob-label") Online
                div(class="col-xs-4 text-center")
                  input(type="text" class="knob" data-readonly="true" value="30" data-width="60" data-height="60" data-fgColor="#39CCCC")
                  div(class="knob-label") In-Store
          div(class="box box-solid bg-green-gradient")
            div(class="box-header")
              i(class="fa fa-calendar")
              h3(class="box-title") Calendar
              div(class="pull-right box-tools")
                div(class="btn-group") 
                  button(type="button" class="btn btn-success btn-sm dropdown-toggle" data-toggle="dropdown")
                    i(class="fa fa-bars")
                  ul(class="dropdown-menu pull-right" role="menu")
                    li
                      a(href="#") Add new event
                    li
                      a(href="#") Clear events
                    li(class="divider")
                    li
                      a(href="#") View calendar
                button(type="button" class="btn btn-success btn-sm" data-widget="collapse")
                  i(class="fa fa-minus")
                button(type="button" class="btn btn-success btn-sm" data-widget="remove")
                  i(class="fa fa-times")
            div(class="box-body no-padding")
              div(id="calendar" style="width: 100%")
            div(class="box-footer text-black")
              div(class="row")
                div(class="col-sm-6")
                  div(class="clearfix") 
                    span(class="pull-left") Task #1
                    small(class="pull-right") 90%
                  div(class="progress xs")
                    div(class="progress-bar progress-bar-green" style="width: 90%;")
                  div(class="clearfix")
                    span(class="pull-left") Task #2
                    small(class="pull-right") 70%
                  div(class="progress xs")
                    div(class="progress-bar progress-bar-green" style="width: 70%;")
                div(class="col-sm-6")
                  div(class="clearfix")
                    span(class="pull-left") Task #3
                    small(class="pull-right") 60%
                  div(class="progress xs")
                    div(class="progress-bar progress-bar-green" style="width: 60%;")
                  div(class="clearfix")
                    span(class="pull-left") Task #4
                    small(class="pull-right") 40%
                  div(class="progress xs") 
                    div(class="progress-bar progress-bar-green" style="width: 40%;")