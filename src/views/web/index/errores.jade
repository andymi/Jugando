extends ../../layout 
block content
  div(class="content-wrapper")
    section(class="content-header")
      h1 Error de Página
    section(class="content")
      div(class="error-page")
        h2(class="headline text-yellow") 404
        div(class="error-content")
          h3 Oops! Página no encontrada
            i(class="fa fa-warning text-yellow")
          p No pudimos encontrar la página que buscabas. Mientras tanto, puede volver al tablero de inicio.
          a(href="/principal" class="btn btn-warning btn-flat") Volver a Inicio
            i(class="fa fa-check")