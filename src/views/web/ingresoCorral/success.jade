table(class="table table-bordred table-striped")
  thead
    tr
      th(class="cell-stat text-center") idIngresoCorral
      th(class="cell-stat text-center") fechaIngreso     
      th(class="cell-stat text-center") horaIngreso
      th(class="cell-stat text-center") observacionIngreso
      th(class="cell-stat text-center") idEmpleado
      th(class="cell-stat text-center") nombreEmpleado
      th(class="cell-stat text-center") fechaCreacion
      th(class="cell-stat text-center") fechaModificacion
      th(class="cell-stat text-center") fechaBorrado
      th(class="cell-stat text-center") Accion
      th(class="cell-stat text-center") Accion
  
  tbody
    for ingresoCorral in ingresoCorrales
      tr
        td(class="text-center")=ingresoCorral.idIngresoCorral
        td(class="text-center")=ingresoCorral.fechaIngreso
        td(class="text-center")=ingresoCorral.horaIngreso        
        td(class="text-center")=ingresoCorral.observacionIngreso
        td(class="text-center")=ingresoCorral.Empleado.idEmpleado 
        if ingresoCorral.Empleado == null  
          td(class="text-center") sin empleado
        else 
          td(class="text-center")=ingresoCorral.Empleado.nombreEmpleado 
        
        td(class="text-center")=ingresoCorral.fechaCreacion 
        td(class="text-center")=ingresoCorral.fechaModificacion
        td(class="text-center")=ingresoCorral.fechaBorrado 
        td
          a(href="/web/ingresoCorral/#{ingresoCorral.idIngresoCorral}" class="btn btn-info" role="button")
            p(data-placement="top" data-toggle="tooltip" title="Edit")
              button(type="button" class="btn btn-info") Editar Ingreso Corral
              
        form(id="topicForm" method="post" action="/web/ingresoCorral/#{ingresoCorral.idIngresoCorral}")
          input(type="hidden" name="_method" value="delete")
          input(type="hidden" name="idIngresoCorral" value='#{ingresoCorral.idIngresoCorral}')               
          td
            a(href="/web/ingresoCorral/#{ingresoCorral.idIngresoCorral}" class="btn btn-info" role="button")
              p(data-placement="top" data-toggle="tooltip" title="Delete")
                button(type="submit" name="committ" value="Form" class="btn btn-info") Borrar Ingreso Corral
a(href="/web/ingresoCorral/cargar" class="btn btn-info" role="button")
  p(data-placement="top" data-toggle="tooltip" title="Add")
    button(type="button" class="btn btn-info") Agregar Ingreso Corral


          
