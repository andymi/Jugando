table(class="table table-bordred table-striped")
  thead
    tr
      th(class="cell-stat text-center") idDetalleIA
      th(class="cell-stat text-center") observacion           
      th(class="cell-stat text-center") idAnimal
      th(class="cell-stat text-center") rpAnimal
      th(class="cell-stat text-center") fechaCreacion
      th(class="cell-stat text-center") fechaModificacion
      th(class="cell-stat text-center") fechaBorrado
      th(class="cell-stat text-center") Accion
      th(class="cell-stat text-center") Accion
  
  tbody
    for detalleIngresoAnimal in detalleIngresoAnimales
      tr
        td(class="text-center")=detalleIngresoAnimal.idDetalleIA
        td(class="text-center")=detalleIngresoAnimal.observacion       
        td(class="text-center")=detalleIngresoAnimal.Animal.idAnimal
        if detalleIngresoAnimal.Animal == null  
          td(class="text-center") sin Animal
        else 
          td(class="text-center")=detalleIngresoAnimal.Animal.rpAnimal        
        td(class="text-center")=detalleIngresoAnimal.fechaCreacion 
        td(class="text-center")=detalleIngresoAnimal.fechaModificacion
        td(class="text-center")=detalleIngresoAnimal.fechaBorrado 
        td
          a(href="/web/detalleIngresoAnimal/editar/#{detalleIngresoAnimal.idDetalleIA}" class="btn btn-info" role="button")
            p(data-placement="top" data-toggle="tooltip" title="Edit")
              button(type="button" class="btn btn-info") Editar Detalle Ingreso 
              
        form(id="topicForm" method="post" action="/web/detalleIngresoAnimal/#{detalleIngresoAnimal.idDetalleIA}")
          input(type="hidden" name="_method" value="delete")
          input(type="hidden" name="idConsumo" value='#{detalleIngresoAnimal.idDetalleIA}')               
          td
            a(href="/web/detalleIngresoAnimal/#{detalleIngresoAnimal.idDetalleIA}" class="btn btn-info" role="button")
              p(data-placement="top" data-toggle="tooltip" title="Delete")
                button(type="submit" name="committ" value="Form" class="btn btn-info") Borrar Detalle Ingreso


          
